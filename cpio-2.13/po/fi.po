msgstr "kohteen ”%s” avaaminen epäonnistui"

#: tests/genfile.c:435
msgid "cannot seek"
msgstr "etsintä epäonnistui"

#: tests/genfile.c:452
#, c-format
msgid "file name contains null character"
msgstr "tiedostonimi sisältää null-merkin"

#: tests/genfile.c:566
#, c-format
